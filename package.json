{
    "name": "imagemin-power-cli",
    "version": "0.3.1",
    "description": "Minify images with imagemin use CLI",
    "keywords": [
        "cli",
        "cli-app",
        "compress",
        "imagemin",
        "gif",
        "image",
        "images",
        "jpeg",
        "jpg",
        "minify",
        "png",
        "svg",
        "webp"
    ],
    "author": "Itgalaxy",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/itgalaxy/imagemin-power-cli.git"
    },
    "homepage": "https://github.com/itgalaxy/imagemin-power-cli",
    "bugs": "https://github.com/itgalaxy/imagemin-power-cli/issues",
    "bin": {
        "imagemin-power": "cli.js"
    },
    "engines": {
        "node": ">=4"
    },
    "files": [
        "cli.js"
    ],
    "dependencies": {
        "arrify": "^1.0.1",
        "file-type": "^3.8.0",
        "fs-extra": "^1.0.0",
        "get-stdin": "^5.0.1",
        "globby": "^6.0.0",
        "imagemin": "^5.0.0",
        "meow": "^3.6.0",
        "ora": "^0.3.0",
        "plur": "^2.1.2",
        "pretty-bytes": "^4.0.0",
        "strip-indent": "^2.0.0",
        "replace-ext": "^1.0.0"
    },
    "devDependencies": {
        "ava": "^0.17.0",
        "babel-cli": "^6.11.0",
        "babel-core": "^6.13.0",
        "babel-preset-es2015": "^6.13.0",
        "babel-preset-stage-0": "^6.5.0",
        "babel-register": "^6.9.0",
        "coveralls": "^2.11.0",
        "eslint": "^3.10.0",
        "eslint-plugin-ava": "^4.0.0",
        "eslint-plugin-import": "^2.2.0",
        "eslint-plugin-itgalaxy": "^30.0.0",
        "eslint-plugin-jsx-a11y": "^3.0.0",
        "eslint-plugin-lodash": "^2.2.0",
        "eslint-plugin-node": "^3.0.0",
        "eslint-plugin-promise": "^3.4.0",
        "eslint-plugin-react": "^6.7.0",
        "eslint-plugin-unicorn": "^1.0.0",
        "execa": "^0.5.0",
        "imagemin-pngquant": "^5.0.0",
        "npmpub": "^3.1.0",
        "npm-run-all": "^3.0.0",
        "nyc": "^10.0.0",
        "pify": "^2.3.0",
        "remark-cli": "^2.0.0",
        "remark-lint": "^5.0.0",
        "remark-preset-lint-itgalaxy": "^3.0.0",
        "rimraf": "^2.5.2"
    },
    "optionalDependencies": {
        "imagemin-gifsicle": "^5.0.0",
        "imagemin-jpegtran": "^5.0.0",
        "imagemin-optipng": "^5.0.0",
        "imagemin-svgo": "^5.0.0"
    },
    "scripts": {
        "coveralls": "nyc report --reporter=text-lcov | coveralls",

        "lint:eslint": "eslint . --ignore-pattern '**/__tests__/**' --ignore-path .gitignore",
        "lint:remark": "remark '{**/*,*}.md' -i .gitignore -f -q",
        "lint": "npm-run-all -l -p lint:**",

        "ava": "nyc ava --verbose '**/__tests__/*.js'",

        "pretest": "npm run lint",
        "test": "npm run ava",

        "release": "npmpub"
    },
    "babel": {
        "presets": [
            "es2015",
            "stage-0"
        ]
    },
    "ava": {
        "require": "babel-register",
        "concurrency": 5,
        "babel": "inherit"
    },
    "eslintConfig": {
        "rules": {
            "import/no-commonjs": "off",

            "node/shebang": "off"
        },
        "extends": [
            "plugin:itgalaxy/ava",
            "plugin:itgalaxy/esnext",
            "plugin:itgalaxy/node"
        ],
        "root": true
    }
}
